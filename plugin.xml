<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cordova_plugin_toastsample" version="1.0.0">
   <name>toastsample</name>
   <js-module name="toastsample" src="www/toastsample.js">
      <clobbers target="cordova.plugins.toastsample" />
   </js-module>
   <platform name="android">
      <config-file parent="/*" target="res/xml/config.xml">
         <feature name="toastsample">
            <param name="android-package" value="cordova_plugin_toastsample.toastsample" />
         </feature>
      </config-file>
      <config-file parent="/*" target="AndroidManifest.xml" />
      <source-file src="src/android/toastsample.java" target-dir="src/cordova_plugin_toastsample/toastsample" />
   </platform>
</plugin>